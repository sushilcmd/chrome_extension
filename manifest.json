{
    "name": "HR Pointer",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+F",
                "mac": "MacCtrl+Shift+F"
            },
            "description": "Opens hello.html"
        }
    },
    "content_scripts": {
        "js": ["jquery-3.js", "jquery.tmpl.min.js", "completeTask.js", "pendingTask.js", "newTask.js", "base.js", "index.js"]
    }
}